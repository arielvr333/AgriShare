<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".RegisterFragment">


    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:text="Fullname"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/regfrag_fullName_txt"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/regfrag_fullName_txt"
        android:layout_width="354dp"
        android:layout_height="38dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        android:background="#8DFFFFFF"
        android:ems="10"
        android:hint="full name"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="12dp"
        android:text="Phone number"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/regfrag_Phonenumber"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/regfrag_userName_txt"
        android:layout_width="343dp"
        android:layout_height="43dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="32dp"
        android:background="#8DFFFFFF"
        android:ems="10"
        android:hint="user name"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/regfrag_fullName_txt" />

    <EditText
        android:id="@+id/regfrag_Phonenumber"
        android:layout_width="346dp"
        android:layout_height="40dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="32dp"
        android:background="#8DFFFFFF"
        android:ems="10"
        android:hint="Phonenumber"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/regfrag_Address" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="12dp"
        android:text="Address"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/regfrag_Address"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/regfrag_password"
        android:layout_width="343dp"
        android:layout_height="59dp"
        android:background="#8DFFFFFF"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintBottom_toTopOf="@+id/regfrag_Address"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.441"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/regfrag_userName_txt"
        app:layout_constraintVertical_bias="0.552" />

    <EditText
        android:id="@+id/regfrag_Address"
        android:layout_width="343dp"
        android:layout_height="43dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="288dp"
        android:layout_marginEnd="32dp"
        android:background="#8DFFFFFF"
        android:ems="10"
        android:hint="Address"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="4dp"
        android:text="user name"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/regfrag_userName_txt"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:text="password"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/regfrag_password"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/regfrag_register_btn"
        android:layout_width="204dp"
        android:layout_height="69dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="32dp"
        android:backgroundTint="#37AC2F"
        android:text="register"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/regfrag_Phonenumber" />

    <Button
        android:id="@+id/regfrag_login_btn"
        style="@style/Widget.MaterialComponents.Button.TextButton"
        android:layout_width="195dp"
        android:layout_height="59dp"
        android:backgroundTint="#37AC2F"
        android:text="login
"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/regfrag_register_btn"
        app:layout_constraintVertical_bias="0.174" />


</androidx.constraintlayout.widget.ConstraintLayout>